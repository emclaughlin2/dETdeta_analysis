executable = CondorMC2024Run.sh
arguments = $(generator) $(centmin) $(centmax) $(tag) $(directory) $(closure)
output = output_logs/output_epos_2024_fb_$(Process).out
when_to_transfer_output = ON_EXIT
error			= output_logs/error_epos_2024_fb_$(Process).err
log = /tmp/egm2153_$(Process).log
queue generator, centmin, centmax, tag, directory, closure from epos_2024_queue.list